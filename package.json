{
	"name": "filmbucket",
	"private": true,
	"scripts": {
		"start": "source .env.sh && meteor run"
	},
	"dependencies": {
		"@babel/runtime": "^7.0.0-beta.51",
		"@quarterto/assert-env": "^1.4.0",
		"@quarterto/below-median": "^1.2.0",
		"boulevard": "^2.3.5",
		"camel-case": "^1.2.2",
		"classnames": "^2.2.5",
		"copy-to-clipboard": "^3.0.2",
		"core-js": "^2.5.7",
		"deep-map-keys": "^1.2.0",
		"lodash": "^4.13.1",
		"lodash.countby": "^4.4.0",
		"lodash.difference": "^4.3.0",
		"lodash.filter": "^4.3.0",
		"lodash.groupby": "^4.4.0",
		"lodash.intersection": "^4.2.0",
		"lodash.keyby": "^4.4.0",
		"lodash.mapvalues": "^4.4.0",
		"lodash.pickby": "^4.4.0",
		"lodash.some": "^4.3.0",
		"meteor-node-stubs": "^0.2.3",
		"react": "^16.12.0",
		"react-addons-pure-render-mixin": "^15.1.0",
		"react-dom": "^16.12.0",
		"semantic-ui-react": "^0.88.2"
	},
	"devDependencies": {
		"@quarterto/eslint-config": "^1.4.0",
		"@quarterto/prettier": "^1.1.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"husky": "^2.3.0",
		"lint-staged": "^8.1.7",
		"prettier": "^1.17.0"
	},
	"eslintConfig": {
		"extends": [
			"@quarterto"
		],
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": "2019"
		},
		"rules": {
			"camelcase": [
				"error",
				{
					"allow": [
						"api_key"
					]
				}
			],
			"meteor/no-session": "off",
			"meteor/audit-argument-checks": "off"
		},
		"settings": {
			"import/resolver": "meteor"
		},
		"env": {
			"meteor": true
		}
	},
	"prettier": "@quarterto/prettier",
	"lint-staged": {
		"*.{js,json}": [
			"prettier --write",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
